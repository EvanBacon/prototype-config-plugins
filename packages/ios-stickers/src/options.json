{
    "title": "@config-plugins/ios-stickers options",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$ref": "#/definitions/Props",
    "definitions": {
        "Props": {
            "type": "object",
            "properties": {
                "stickers": {
                    "type": "array",
                    "items": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "$ref": "#/definitions/Sticker"
                            }
                        ]
                    }
                },
                "icon": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "stickerBundleId": {
                    "type": "string"
                },
                "columns": {
                    "type": "number",
                    "enum": [
                        2,
                        3,
                        4
                    ]
                }
            },
            "additionalProperties": false
        },
        "Sticker": {
            "type": "object",
            "properties": {
                "image": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "accessibilityLabel": {
                    "type": "string"
                }
            },
            "required": [
                "image"
            ],
            "additionalProperties": false
        }
    }
}